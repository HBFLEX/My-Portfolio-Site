---
import skills from '../data/skillData.js'
---

<div class="mt-5">
    <h1 class="text-center mb-5 pt-3">Skills</h1>
    <div class="row">

       {
        skills.map(skill => (
            <div class="col-md-3">
                <div class={`${skill.class} mb-3`} data-aos="zoom-out-up">
                    {skill.name}
                </div>
        </div>
        ))
       }
       
    </div>
</div>